{
    "manifest_version": 2,
    "name": "Unfollow Steam Currators",
    "version": "1.0",
    "description": "A Browser extension to unfollow steam curators.",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  